  "example_calls": {
    "get_balance": {
      "method": "POST",
      "url": "https://mainnet.helius-rpc.com/?api-key=YOUR_KEY",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "jsonrpc": "2.0",
        "id": 1,
        "method": "getBalance",
        "params": ["PUBLIC_KEY_ADDRESS"]
      }
    },
    "get_transactions": {
      "method": "POST", 
      "url": "https://mainnet.helius-rpc.com/?api-key=YOUR_KEY",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "jsonrpc": "2.0",
        "id": 1,
        "method": "getSignaturesForAddress",
        "params": ["PUBLIC_KEY_ADDRESS", {"limit": 10}]
      }
    },
    "webhook_subscription": {
      "method": "POST",
      "url": "https://mainnet.helius-rpc.com/v0/webhooks?api-key=YOUR_KEY",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer YOUR_KEY"
      },
      "body": {
        "webhook": "https://your-server.com/webhook/transaction-alert",
        "addresses": ["PUBLIC_KEY_ADDRESS"],
        "transactionTypes": ["TRANSFER", "SWAP"],
        "accountBuiltins": ["SYSTEM_PROGRAM"],
        "accountProgramIds": []
      }
    }
  },
  "security_notes": {
    "api_keys": "Replace YOUR_KEY dengan actual API key dari provider",
    "webhook_urls": "Update webhook URLs dengan actual endpoints",
